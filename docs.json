{
  "$schema": "https://mintlify.com/schema.json",
  "name": "enovuh",
  "theme": "mint",
  "logo": {
    "dark": "https://cdn.discordapp.com/avatars/1346591277944148119/e8b7a428a325ddb84d2478210c272915.png?size=1024",
    "light": "https://cdn.discordapp.com/avatars/1346591277944148119/e8b7a428a325ddb84d2478210c272915.png?size=1024"
