{
  "$schema": "https://mintlify.com/schema.json",
  "name": "enovuh",
  "theme": "mint",
  "logo": {
    "dark": "https://cdn.discordapp.com/avatars/1346591277944148119/e8b7a428a325ddb84d2478210c272915.png?size=1024",
    "light": "https://cdn.discordapp.com/avatars/1346591277944148119/e8b7a428a325ddb84d2478210c272915.png?size=1024"
  },
  "favicon": "https://cdn.discordapp.com/avatars/1346591277944148119/e8b7a428a325ddb84d2478210c272915.png?size=256",
  "colors": {
    "primary": "#6D28D9",
    "light": "#A78BFA",
    "dark": "#4C1D95"
  },
  "modeToggle": {
    "default": "dark",
    "isHidden": true
  },
  "topbarLinks": [
    {
      "name": "support server",
      "url": "https://discord.gg/enovuh"
    }
  ],
  "topbarCtaButton": {
    "name": "invite bot",
    "url": "https://enovuh.lol/invite"
  },
  "redirects": [
    {
      "source": "/commands",
      "destination": "https://enovuh.lol/commands"
    },
    {
      "source": "/status",
      "destination": "https://enovuh.lol/status"
    },
    {
      "source": "/invite",
      "destination": "https://enovuh.lol/invite"
    },
    {
      "source": "/discord",
      "destination": "https://discord.gg/enovuh"
    }
  ],
  "navigation": {
    "groups": [
      {
        "group": "start here",
        "icon": "play",
        "pages": [
          "index",
          "intro/quickstart"
        ]
      },
      {
        "group": "core modules",
        "icon": "cube",
        "pages": [
          "modules/antinuke",
          "modules/automod",
          "modules/verification",
          "modules/xp"
        ]
      },
      {
        "group": "management tools",
        "icon": "toolbox",
        "pages": [
          "tools/roles",
          "tools/access",
          "tools/events",
          "tools/channels",
          "tools/greetings",
          "tools/tasks"
        ]
      },
      {
        "group": "entertainment",
        "icon": "gamepad",
        "pages": [
          "fun/music",
          "fun/minigames",
          "fun/images",
          "fun/reaction-roles",
          "fun/giveaways"
        ]
      },
      {
        "group": "social connections",
        "icon": "share-nodes",
        "pages": [
          "social/spotify",
          "social/twitter",
          "social/twitch",
          "social/youtube"
        ]
      },
      {
        "group": "bot settings",
        "icon": "sliders",
        "pages": [
          "settings/prefix",
          "settings/permissions",
          "settings/logging"
        ]
      },
      {
        "group": "help center",
        "icon": "circle-question",
        "pages": [
          "help/dashboard",
          "help/common-errors",
          "help/contact"
        ]
      }
    ]
  },
  "footerSocials": {
    "discord": "https://discord.gg/enovuh",
    "github": "https://github.com/enovuh-bot",
    "twitter": "https://twitter.com/enovuh"
  }
}