{
  "$schema": "https://mintlify.com/schema.json",
  "name": "jugg",
  "theme": "mint",
  "logo": {
    "dark": "https://jugg.lol/branding/jugg-dark.svg",
    "light": "https://jugg.lol/branding/jugg-light.svg"
  },
  "favicon": "favicon.svg",
  "colors": {
    "primary": "#6D28D9",
    "light": "#A78BFA",
    "dark": "#4C1D95"
    }
  },
  "modeToggle": {
    "default": "dark",
    "isHidden": true
  },
  "topbarLinks": [
    {
      "name": "Support Server",
      "url": "https://jugg.lol/support"
    }
  ],
  "topbarCtaButton": {
    "name": "Home",
    "url": "https://jugg.lol"
  },
  "redirects": [
    {
      "source": "/commands",
      "destination": "https://jugg.lol/commands"
    },
    {
      "source": "/status",
      "destination": "https://jugg.lol/status"
    },
    {
      "source": "/invite",
      "destination": "https://jugg.lol/invite"
    },
    {
      "source": "/discord",
      "destination": "https://discord.gg/jugg"
    }
  ],
  "navigation": [
    {
      "group": "Overview",
      "pages": [
        "overview/introduction",
        "overview/features",
        "overview/installation"
      ]
    },
    {
      "group": "Protection",
      "pages": [
        "protection/antinuke",
        "protection/antiraid",
        "protection/security"
      ]
    },
    {
      "group": "Server Configuration",
      "pages": [
        {
          "group": "Roles",
          "icon": "users-gear",
          "pages": [
            "configuration/roles/vanity",
            "configuration/roles/booster",
            "configuration/roles/reaction",
            "configuration/roles/button"
          ]
        },
        {
          "group": "Messages",
          "icon": "message",
          "pages": [
            "configuration/messages/system",
            "configuration/messages/responder",
            "configuration/messages/timer"
          ]
        },
        "configuration/starboard",
        "configuration/voicemaster",
        "configuration/level-rewards",
        "configuration/bump-reminder",
        "configuration/reaction-triggers",
        "configuration/command-aliases",
        "configuration/logging"
      ]
    },
    {
      "group": "Settings",
      "pages": [
        "settings/customization",
        "settings/commands",
        "settings/dashboard"
      ]
    },
    {
      "group": "Miscellaneous",
      "pages": [
        "miscellaneous/music",
        "miscellaneous/webhook",
        "miscellaneous/giveaway",
        "miscellaneous/counters"
      ]
    },
    {
      "group": "Integrations",
      "pages": [
        "integrations/spotify",
        "integrations/lastfm",
        "integrations/fortnite",
        "integrations/social-notifications"
      ]
    },
    {
      "group": "API",
      "pages": [
        "api/developer-intro",
        "api/endpoints",
        "api/authentication",
        "api/rate-limiting"
      ]
    },
    {
      "group": "Common Issues",
      "pages": [
        "common-issues/ios-dash",
        "common-issues/permissions",
        "common-issues/server-removal"
      ]
    }
  ],
  "footerSocials": {
    "discord": "https://discord.gg/jugg",
    "github": "https://github.com/jugg-bot",
    "twitter": "https://twitter.com/jugg"
  }
}