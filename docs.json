{
  "$schema": "https://mintlify.com/schema.json",
  "name": "enovuh",
  "theme": "mint",
  "logo": {
    "dark": "https://enovuh.lol/branding/enovuh-dark.svg",
    "light": "https://enovuh.lol/branding/enovuh-light.svg"
  },
  "favicon": "favicon.svg",
  "colors": {
    "primary": "#6D28D9",
    "light": "#A78BFA",
    "dark": "#4C1D95"
  },
  "modeToggle": {
    "default": "dark",
    "isHidden": true
  },
  "topbarLinks": [
    {
      "name": "support server",
      "url": "https://enovuh.lol/support"
    }
  ],
  "topbarCtaButton": {
    "name": "home",
    "url": "https://enovuh.lol"
  },
  "redirects": [
    {
      "source": "/commands",
      "destination": "https://enovuh.lol/commands"
    },
    {
      "source": "/status",
      "destination": "https://enovuh.lol/status"
    },
    {
      "source": "/invite",
      "destination": "https://enovuh.lol/invite"
    },
    {
      "source": "/discord",
      "destination": "https://discord.gg/enovuh"
    }
  ],
  "navigation": {
    "languages": [],
    "versions": [],
    "tabs": [],
    "dropdowns": [],
    "anchors": [],
    "groups": [
      {
        "name": "overview",
        "pages": [
          "overview/introduction",
          "overview/protection",
          "overview/configuration"
        ]
      },
      {
        "name": "protection",
        "pages": [
          "protection/antinuke",
          "protection/antiraid",
          "protection/joingate",
          "protection/automod"
        ]
      },
      {
        "name": "server configuration",
        "pages": [
          "configuration/roles",
          "configuration/messages",
          "configuration/starboard",
          "configuration/voicemaster",
          "configuration/levels",
          "configuration/bumpreminder",
          "configuration/autoresponses",
          "configuration/aliases",
          "configuration/logging"
        ]
      },
      {
        "name": "commands",
        "pages": [
          "commands/protection",
          "commands/security",
          "commands/utility",
          "commands/configuration",
          "commands/levels",
          "commands/music",
          "commands/support",
          "commands/fun",
          "commands/social"
        ]
      },
      {
        "name": "features",
        "pages": [
          "features/music",
          "features/economy",
          "features/leveling",
          "features/counters"
        ]
      },
      {
        "name": "integrations",
        "pages": [
          "integrations/spotify",
          "integrations/lastfm",
          "integrations/social"
        ]
      },
      {
        "name": "common issues",
        "pages": [
          "common-issues/permissions",
          "common-issues/errors",
          "common-issues/troubleshooting"
        ]
      }
    ],
    "pages": []
  },
  "footerSocials": {
    "discord": "https://discord.gg/enovuh",
    "github": "https://github.com/enovuh-bot",
    "twitter": "https://twitter.com/enovuh"
  }
}